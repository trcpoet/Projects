CREATE TABLE Employee ( EmployeeID INTEGER PRIMARY KEY, EmployeeName TEXT NOT NULL, DepartmentID INTEGER NOT NULL, Rank INTEGER NOT NULL, Salary INTEGER NOT NULL,	FOREIGN KEY(DepartmentID) REFERENCES Department(DepartmentID),   		FOREIGN KEY(Rank) REFERENCES Positions(Rank));CREATE TABLE Department (DepartmentID INTEGER PRIMARY KEY,DepartmentName TEXT NOT NULL);CREATE TABLE Positions (Rank INTEGER PRIMARY KEY,PositionTitle TEXT NOT NULL);      insert into Employee values (1, "Bruce Wayne", 1, 2, 90000);insert into Employee values (2, "Hulk", 2, 3, 70000);insert into Employee values (3, "Bruce Lee", 1, 4, 12345);insert into Employee values (4, "Ronaldo", 3, 5, 50000);insert into Employee values (5, "Messi", 2, 1, 40000);   		insert into Department values (1, "Hero");insert into Department values (2, "Superhero");insert into Department values (3, "Fighter");insert into Department values (4, "Footballer");insert into Department values (5, "The Goat");   		insert into Positions values (1,"CEO");insert into Positions values (2,"CFO");insert into Positions values (3,"Manager");insert into Positions values (4, "Supervisor");insert into Positions values (5,"Employee");select e.EmployeeName, p.PositionTitle, p.Rank, d.DepartmentID from Employee e, Department d, Positions p where e.EmployeeID=d.DepartmentID AND e.EmployeeID=p.Rank;